{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "BikeRentalApp",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZWS16_T1_BIKE_RENTAL_SRV": {
				"uri": "/AbapConnection/sap/opu/odata/sap/ZWS16_T1_BIKE_RENTAL_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZWS16_T1_BIKE_RENTAL_SRV/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": false,
			"tablet": false,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "BikeRentalApp.view.login",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "BikeRentalApp.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneTime",
					"defaultCountMode": "Request"
				},
				"dataSource": "ZWS16_T1_BIKE_RENTAL_SRV",
				"preload": true
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "BikeRentalApp.view",
				"controlId": "BikeRentalApp",
				"controlAggregation": "pages",
				"transition": "slide"
			},
			"routes": [{
				"pattern": "",
				"name": "appLogin",
				"target": "login"
			}, {
				"pattern": "dashboard",
				"name": "dashboard",
				"target": "dashboard"
			}, {
				"pattern": "searchbike",
				"name": "searchbike",
				"target": "searchbike"
			}, {
				"pattern": "choosebike",
				"name": "choosebike",
				"target": "choosebike"
			}, {
				"pattern": "myrides",
				"name": "myrides",
				"target": "myrides"
			}, {
				"pattern": "information",
				"name": "information",
				"target": "information"
			}, {
				"pattern": "tariff",
				"name": "tariff",
				"target": "tariff"
			}, {
				"pattern": "reportissues",
				"name": "reportissues",
				"target": "reportissues"
			}],
			"targets": {
				"login": {
					"viewName": "searchbike",
					"viewLevel": 1
				},
				"dashboard": {
					"viewName": "dashboard",
					"viewLevel": 1
				},
				"searchbike": {
					"viewName": "searchbike",
					"viewLevel": 1
				},
				"choosebike": {
					"viewName": "choosebike",
					"viewLevel": 1
				},
				"myrides": {
					"viewName": "myrides",
					"viewLevel": 1
				},
				"information": {
					"viewName": "information",
					"viewLevel": 1
				},
				"tariff": {
					"viewName": "tariff",
					"viewLevel": 1
				},
				"reportissues": {
					"viewName": "reportissues",
					"viewLevel": 1
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		}
	}
}