<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:gmaps="openui5.googlemaps" 
xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="BikeRentalApp.controller.dashboard">
	<App>
		<pages>
			<Page title="Welcome!" class="custompage">
				<content>
					<Page showHeader="false" enableScrolling="true">
						<TileContainer id="dashboardtiles" tiles="{/TileCollection}" class="dashboard">
							<StandardTile icon="sap-icon://{icon}" type="{type}" number="{number}" numberUnit="{numberUnit}" 
							class="customtiles" press="tilepress" title="{title}" info="{info}" infoState="{infoState}"/>
						</TileContainer>
					</Page>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>